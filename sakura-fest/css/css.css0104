.tabs{
}
.tabs__btns:after{
	content:"";
	display: block;
	clear:both;
}
.tabs__e{
	position:relative;
	padding:52px 20px 45px 90px;
	font-family:gp5;
	font-size: 18px;
	line-height: 119%;
	text-transform: uppercase;
	border: 1px solid #000000;
	display:block;
}
.tabs__e:nth-child(1){
	display:none;
}
.tabs__e:nth-child(2){
	border-radius:50px 50px 0 0;
}
.tabs__e:last-child{
	border-radius:0 0 50px 50px;
}
.tabs__e:not(.act){
	color: black;
	border-bottom:1px solid;
}
.tabs__e:not(.act) .tabs__e-1{
	border-bottom:1px solid transparent;
}
.tabs__e:not(.act):hover{
	cursor:pointer;
}
.tabs__e.act{
	background:url(../img/arw1-1.svg)no-repeat right 90px center;
}
.tabs__e-c,
.tabs__in-c {
    width: 48%;
}
.tabs__in{
	border-radius:50px;
	border: 1px solid #000000;
	padding:52px 93px;
	font-family: gp5;
	font-size: 18px;
	line-height: 119%;
	height: 100%;
	text-transform: uppercase;
}
.tabs__in:nth-child(1) {
    border: 0;
    background: url(../img/b2-1.jpg)no-repeat center;
    background-size: 100% 100%;
}
.tabs__in:not(.act){
	display:none;
}

.tabs__in span {
	font-family: 'gp9';
}

.tabs__in-black {
	font-family: 'gp7';
}

.tabs__in-black span {
	font-size: 12px;
	font-family: 'gp3';
}

