.crl1.owl-theme .owl-dots .owl-dot.active:before{
	content: '';
    display: block;
    border-radius: 50%;
    position: absolute;
    border: 4px solid #fff;
    width: 97px;
    height: 97px;
    top: -2px;
    left: -2px;
    line-height: 87px;
