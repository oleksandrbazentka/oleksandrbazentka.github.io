@font-face {
  font-family: 'gp3';
  src: url("../fonts/GothamPro-Light.woff") format("woff"); }

@font-face {
  font-family: 'gp';
  src: url("../fonts/GothamPro.woff2") format("woff2"); }

@font-face {
  font-family: 'gp5';
  src: url("../fonts/GothamPro-Medium.woff2") format("woff2"); }

@font-face {
  font-family: 'gp7';
  src: url("../fonts/GothamPro-Bold.woff2") format("woff2"); }

@font-face {
  font-family: 'gp9';
  src: url("../fonts/GothamPro-Black.woff") format("woff"); }

@font-face {
  font-family: 'm3';
  src: url("../fonts/MAK.woff") format("woff"); }

@font-face {
  font-family: 'm7';
  src: url("../fonts/MAK-bold.woff") format("woff"); }

* {
  box-sizing: border-box;
  margin: 0;
  padding: 0;
  border: 0;
  outline: 0; 
}
body{
	font-family:gp,sans-serif;
	color:#2f2f2f;
	font-size:14px;
}

a {
  text-decoration: none;
  color:#2f2f2f;
  cursor: pointer;
}

button {
	cursor: pointer;
}

li {
  list-style: none; 
}
table {
  border-collapse: collapse; 
}
.hide {
  display: none !important; 
}

.hr-a {
	transition: all 0.3s ease;
}

.hr-a:hover {
	text-decoration: underline;
}

.hr-hover {
	transition: all 0.5s ease;
}

.burger {
	transition: all 0.6s ease;
}

.burger:hover {
	box-shadow: 0px 0px 20px black;
}

.btn-hover-white {
	transition: all 0.6s ease;
}

.btn-hover-white:hover {
	background-color: white;
	color: #7D6162;
}

.bn-hover-green {
	transition: all 0.6s ease;
}

.bn-hover-green:hover {
	background: #f4b9b8;
}

.bn-hover-purp:hover {
	background: #BD66A4;
}

.bn-hover-brown:hover {
	background: #852F30;
}

.bn-hover-brown {
	transition: all .6s ease;
}

.b7-a:hover {
	text-decoration: underline;
}

.underline:hover {
	text-decoration: underline;
}

.underline {
	cursor: pointer;
}

.bn-hover-purp {
	transition: all .6s ease;
}

.header__burger {
	width: 40px;
	height: 28px;
	position: relative;
	display: none;
}

.c {
	margin-right:auto;
	margin-left:auto;
	overflow: visible;
	overflow-x: hidden;
}
.c1{
	max-width:1580px;
	margin-right:auto;
	margin-left:auto;
}
.dn{
	display:none !important;
}

.upper {
	text-transform: uppercase;
}

